<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" 
xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" 
id="WebApp_ID" version="3.1">
  <display-name>FormCreation</display-name>
  
  <context-param>
	<param-name>dClass</param-name>
	<param-value>com.mysql.jdbc.Driver</param-value>
	</context-param>
	<context-param>
	<param-name>url</param-name>
	<param-value>jdbc:mysql://localhost:3306/prodapt</param-value>
	</context-param>
	<context-param>
	<param-name>username</param-name>
	<param-value>root</param-value>
	</context-param>
	<context-param>
	<param-name>password</param-name>
	<param-value>root</param-value>
	</context-param>
	
	
	<servlet>
	<servlet-name>regser</servlet-name>
	<servlet-class>edu.ialabs.Form_creation</servlet-class>
	</servlet>
	<servlet-mapping>
	<servlet-name>regser</servlet-name>
	<url-pattern>/reg</url-pattern>
	</servlet-mapping>
	
	
	
	<servlet>
	<servlet-name>ddd</servlet-name>
	<servlet-class>edu.ialabs.Login_page</servlet-class>
	</servlet>
	<servlet-mapping>
	<servlet-name>ddd</servlet-name>
	<url-pattern>/log</url-pattern>
	</servlet-mapping>
</web-app>
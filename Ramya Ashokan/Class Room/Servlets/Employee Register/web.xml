<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
  <display-name>EmployeeWeb</display-name>
  <servlet>
  <servlet-name>EmployeeReg</servlet-name>
  <servlet-class>com.employee.Employee</servlet-class>
  <init-param>
  <param-name>dClass</param-name>
  <param-value>com.mysql.cj.jdbc.Driver</param-value>
  </init-param>  
  <init-param>
  <param-name>url</param-name>
  <param-value>jdbc:mysql://localhost:3306/prodapt</param-value>
  </init-param>
   <init-param>
  <param-name>uname</param-name>
  <param-value>root</param-value>
  </init-param>
   <init-param>
  <param-name>pswd</param-name>
  <param-value>root</param-value>
  </init-param>
  
  </servlet>
  <servlet-mapping>
  <servlet-name>EmployeeReg</servlet-name>
  <url-pattern>/reg</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
    <welcome-file>emp.html</welcome-file>
  
  </welcome-file-list>
</web-app>
<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
  <display-name>webapp3</display-name>
  
  <context-param>
  <param-name>dclass</param-name>
  <param-value>com.mysql.cj.jdbc.Driver</param-value>
  </context-param>
  
  <context-param>
  <param-name>url</param-name>
  <param-value>jdbc:mysql://localhost:3306/employee</param-value>
  </context-param>
  
  <context-param>
  <param-name>uname</param-name>
  <param-value>root</param-value>
  </context-param>
  
  
  <context-param>
  <param-name>pword</param-name>
  <param-value>root</param-value>
  </context-param>
  
  <servlet>
  <servlet-name>login</servlet-name>
  <servlet-class>app3.Serve</servlet-class>
  </servlet>
  <servlet-mapping>
  <servlet-name>login</servlet-name>
  <url-pattern>/log</url-pattern>
  </servlet-mapping>
  
  <servlet>
  <servlet-name>regi</servlet-name>
  <servlet-class>app3.form</servlet-class>
  </servlet>
  <servlet-mapping>
  <servlet-name>regi</servlet-name>
  <url-pattern>/reg</url-pattern>
  </servlet-mapping>
  
  
</web-app>
